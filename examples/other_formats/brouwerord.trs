(FUN
  lim : (n -> o) -> o
  zeroN : n
  sucN : n -> n
  zero : o
  suc : o -> o
  rec : o -> a -> (o -> a -> a) -> ((n -> o) -> (n -> a) -> a) -> a
)
(VAR
  x : o
  u : a
  V : o -> a -> a
  Y : n -> o
)
(RULES
  rec zero u V W -> u,
  rec (suc x) u V W -> V x (rec x u V W),
  rec (lim Y) u V W -> W Y (\n. rec (Y n) u V W)
)